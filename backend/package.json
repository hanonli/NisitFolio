    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^8.0.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^7.1.0",
    "@types/bcrypt": "^5.0.0",
